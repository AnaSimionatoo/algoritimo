programa {
  funcao inicio() {
    
    //Declaração de variaveís
    real n1, n2, area

    //Entrada
    escreva("Digite o comprimento do terreno:")
    leia(n1)
    entrada("Digite a largura do terreno:")
    leia(n2)

    //Processamento
    area= n1 * n2

    //Saída
    escreva("A area do terreno é :", area)
  }
}
