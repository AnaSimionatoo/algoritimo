programa {
  funcao inicio() {

    //declaração de variáveis
    real n1, n2, produto

    //entrada
    escreva("qual valor do produto?: ")
    leia(n1)
    escreva("qual a quantidade do produto?: ")
    leia(n2)

    //processamento
    produto = n1 * n2

    //saída
    escreva("a valor total será: ", produto)

  }
}
