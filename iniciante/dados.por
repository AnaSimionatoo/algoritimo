programa {
  funcao inicio() {
    //declaração de variaveis
    cadeia n
    real i
    real c
    real t

    //Entrada
    escreva("Digite seu nome:")
    leia(n)

    escreva("Digite sua idade:")
    leia(i)

    escreva("digite seu cpf:")
    leia(c)

    escreva("digite seu telefone:")
    leia(t)

    //Saída
    escreva("nome:",n,"\nidade:",i, "\ncpf ",c,"\ntelefone: " ,t)
  }
}
